{
  "name": "seal-blog",
  "version": "0.3.0-rc1",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/sdk",
    "packages/server",
    "packages/*"
  ],
  "scripts": {
    "start": "yarn workspace @seal-blog/server run start",
    "start:client": "yarn workspace @seal-blog/client run start",
    "test": "yarn workspaces run test",
    "fmt": "yarn workspaces run fmt",
    "lint": "yarn workspaces run lint",
    "build": "yarn workspaces run build",
    "web": "yarn workspace @seal-blog/server ts-node-dev src/view.ts",
    "artifact:server": "cp packages/contract/artifacts/contracts/NaiveFriends721.sol/NaiveFriends721.json packages/server/src/configs/blockchain/contract-artifact.json",
    "artifact:client": "cp packages/contract/artifacts/contracts/NaiveFriends721.sol/NaiveFriends721.json packages/client/src/configs/blockchain/contract-artifact.json",
    "artifact": "yarn artifact:server && yarn artifact:client"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "ava": "^4.2.0",
    "prettier": "^2.4.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  }
}
