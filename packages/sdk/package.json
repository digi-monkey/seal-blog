{
  "name": "@seal-blog/sdk",
  "version": "0.2.0",
  "license": "MIT",
  "main": "lib/index",
  "scripts": {
    "start": "ts-node-dev src/index.ts",
    "test": "ava",
    "fmt": "prettier --write src/**/*.ts \"src/*.ts\" tests/*.ts package.json tsconfig.json webpack.config.js",
    "lint": "eslint -c ../../.eslintrc.js \"src/**/*.ts\" \"src/*.ts\" \"tests/**/*.ts\"",
    "build": "tsc",
    "bundle": "webpack",
    "web": "ts-node-dev src/view.ts",
    "pub": "yarn build && yarn bundle && yarn publish"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "/lib",
    "/bundle"
  ],
  "ava": {
    "extensions": [
      "js",
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "dependencies": {
    "@metamask/eth-sig-util": "^4.0.1",
    "axios": "^0.27.2",
    "crypto-js": "^4.1.1",
    "ethereumjs-util": "^7.1.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "buffer": "^6.0.3",
    "eslint": "^8.1.0",
    "ts-loader": "^9.3.0",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  }
}
