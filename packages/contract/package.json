{
  "name": "@seal-blog/contract",
  "version": "0.4.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean": "hardhat clean",
    "fmt": "prettier --write \"src/*\" \"scripts/*\" \"test/*\" \"contracts/*.sol\" \"contracts/**/*.sol\" \"*.js\" \"*.json\" \"*.ts\"",
    "lint": "eslint -c ./.eslintrc.js \"src/*\" \"test/*\" \"scripts/*\" && solhint contracts/*.sol contracts/**/*.sol",
    "avatar": "ts-node src/avatar.ts",
    "compile": "hardhat compile",
    "test": "hardhat test",
    "build": "echo 'no build script'",
    "coverage": "hardhat coverage --solcoverjs ./.solcover.js",
    "deploy:hardhat": "hardhat run --network hardhat scripts/deploy.ts",
    "deploy:polyjuice": "hardhat run --network polyjuice scripts/deploy.ts",
    "deploy:polyjuice-testnet": "hardhat run --network polyjuiceTestnet scripts/deploy.ts",
    "deploy:polyjuice-mainnet": "hardhat run --network polyjuiceMainnet scripts/deploy.ts"
  },
  "devDependencies": {
    "@metamask/eth-sig-util": "^4.0.1",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^2.1.3",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@openzeppelin/contracts": "^4.4.2",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.3.0",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.4.13",
    "chai": "^4.2.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "ethereum-waffle": "^3.0.0",
    "ethers": "^5.0.0",
    "hardhat": "^2.6.8",
    "hardhat-gas-reporter": "^1.0.4",
    "prettier-plugin-solidity": "^1.0.0-beta.13",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.16",
    "typechain": "^5.1.2"
  },
  "dependencies": {
    "@dicebear/avatars": "^4.10.2",
    "@dicebear/croodles": "^4.10.2",
    "@dicebear/open-peeps": "^4.10.2"
  }
}
